--bowtie2Index '/data/tmp/nservant/asmapping_testop/CAST_EiJ_129S1_SvImJ_bowtie2_index/' --nmask --outdir /data/tmp/nservant/asmap_bwt2_nmask \
-w /data/tmp/nservant/asmap_bwt2_nmask/work" | qsub -N  "asmap_bwt2_nmask"

